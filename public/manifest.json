{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "content_scripts": [
    {
      "matches": [
        "https://waterlooworks.uwaterloo.ca/myAccount/dashboard.htm"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": false,
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
    "activeTab"
  ],
  "optional_permissions": [
  ],
  "host_permissions": [
    "http://localhost:8000/*"
  ]
}